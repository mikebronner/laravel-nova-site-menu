{
    "name": "genealabs/laravel-nova-site-menu",
    "description": "Drop-in solution to create dynamic navigation menus.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "proprietary",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "path",
            "url": "../laravel-overridable-model"
        }
    ],
    "require": {
        "genealabs/laravel-overridable-model": "*",
        "illuminate/support": "^10.0|^11.0",
        "laravel/nova": "^4.0",
        "optimistdigital/nova-sortable": "*",
        "symfony/thanks": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "GeneaLabs\\LaravelNovaSiteMenu\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GeneaLabs\\LaravelNovaSiteMenu\\Providers\\Service",
                "GeneaLabs\\LaravelNovaSiteMenu\\Providers\\Nova"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
